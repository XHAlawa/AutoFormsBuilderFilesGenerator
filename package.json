{
  "type": "commonjs",
  "name": "angular-formsbuilder-gen",
  "version": "1.0.28",
  "main": "index.js",
  "keywords": [
    "angular",
    "openapi",
    "codegen",
    "formbuilder"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/XHAlawa/AutoFormsBuilderFilesGenerator.git"
  },
  "author": "Ahmed Halawa",
  "license": "ISC",
  "description": "Auto Generate Forms Builder Files For Angualr 6+ Base On Swagger Generated Scheme",
  "bin": {
    "ng-frmGenerator": "./bin/src/index.js"
  },
  "devDependencies": {
    "ts-node": "^10.9.1"
  },
  "files": [
    "bin/src/*.js",
